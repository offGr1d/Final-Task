public class TestSandwich {
    public static void main(String[] args) {
      Sandwich sandwich1 = new Sandwich("White bread", "Ham and cheese", 200);
      Sandwich sandwich2 = new Sandwich("Whole wheat bread", "Turkey and avocado", 250);
      Sandwich sandwich3 = new Sandwich("Multigrain bread", "Chicken and bacon", 50);

      sandwich1.displaySandwichData();
      sandwich2.displaySandwichData();
      sandwich3.displaySandwichData();
    }
}
